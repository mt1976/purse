<bean id="FXSPOTMarketUSDZAR--SP-Market-Filter" class="com.eurobase.ratemanager.processing.filter.DatumFilterCollectionImpl">
  <constructor-arg index="0">
    <value>FXSPOTMarketUSDZAR--SP-Market-Filter</value>
  </constructor-arg>
  <property name="processes">
    <list>
      <ref bean="FXSPOTMarketUSDZAR--SP-Market-rangeFilter"/>
      <ref bean="FXSPOTMarketUSDZAR--SP-Market-spreadFilter"/>
      <ref bean="FXSPOTMarketUSDZAR--SP-Market-gradientFilter"/>
    </list>
  </property>
  <property name="writer">
    <ref bean="FilterStore-FXSPOTMarketUSDZAR--SP-Market"/>
  </property>
  <property name="rateBean">
    <ref bean="FXSPOTMarketUSDZAR--SP-Market-RateBean"/>
  </property>
</bean>

