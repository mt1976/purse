<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="persister-table.xsl" type="text/xsl"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                           http://www.springframework.org/schema/aop 
                           http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
                           http://www.springframework.org/schema/context 
                           http://www.springframework.org/schema/context/spring-context-2.5.xsd">

<bean id="FXSPOTMarketUSDCYP-SP-Market-Blender" class="com.eurobase.ratemanager.processing.blender.LatestBlend">
  <constructor-arg index="0">
    <value>FXSPOTMarketUSDCYP-SP-Market-Blender</value>
  </constructor-arg>
  <property name="minimumSpread">
    <value>${default_FX_SpotMinSpread}</value>
  </property>
  <property name="maximumSpread">
    <value>${default_FX_SpotMaxSpread}</value>
  </property>
  <property name="writer">
    <ref bean="BlenderStore-FXSPOTMarketUSDCYP-SP-Market"/>
  </property>
  <property name="includeIq">
    <value>true</value>
  </property>
  <property name="factory">
    <ref bean="DefaultRateDatumFactory" />
  </property>
  <property name="auditor">
    <ref bean="auditor" />
  </property>
</bean>

</beans>

