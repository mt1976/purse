<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="persister-table.xsl" type="text/xsl"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                           http://www.springframework.org/schema/aop 
                           http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
                           http://www.springframework.org/schema/context 
                           http://www.springframework.org/schema/context/spring-context-2.5.xsd">

<bean id="FXSPOTMarketEURTRY-SP-Market-Publisher" class="com.eurobase.ratemanager.external.out.RatePublisher">
  <constructor-arg index="0">
    <value>FXSPOTMarketEURTRY-SP-Market-Publisher</value>
  </constructor-arg>
  <property name="rateBean">
    <ref bean="FXSPOTMarketEURTRY-SP-Market-RateBean"/>
  </property>
  <property name="service">
    <ref bean="publisherService"/>
  </property>
  <property name="auditor">
    <ref bean="auditor"/>
  </property>
</bean>

</beans>
